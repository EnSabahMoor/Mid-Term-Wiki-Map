<!DOCTYPE html>
<html lang="en">
  <head>
    <title>WIKI MAP</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>

    <!-- add google map using google api key -->
     <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY&callback=initMap">
    </script>


  </head>

  <body>

  <!-- add google map on the cover page -->
    <div id='map' class='coverPageMap'></div>
     <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>

  <!-- add nav bar include home, login, logout , user info button -->
   <nav id="nav-bar">

     <button class='toHome' type='button'>HOME</button>
     <button class='toLogin' type='button'>LOGIN</button>
     <button class='toLogout' type='button'>LOGOUT</button>
     <button class='toRegister' type='button'>REGISTER</button>
     <button class='toCreateMap' type='button'>CREATE MAP</button>
     <button class='toUserPro' type='button'>USER PROFILE</button>

     <!-- login and register form -->
     <form class='login' action='/login' method='POST'>
      <h2>LOGIN</h2>
      Username:<input name='username'>
      Email:<input name='email' placeholder="name@example.com">
      Password:<input name='password'>
    </form>
     <form class='register' action='/register' method='POST'>
      <h2>REGISTER</h2>
      Username:<input name='username'>
      Email:<input name='email' placeholder="name@example.com">
      Password:<input name='password'>
    </form>


   </nav>





</body>
  <footer>

  </footer>
</html>
